import 'package:flutter/material.dart';

void main() {
  runApp(MyApp());
}

class MyApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      home: GanjilGenapPage(),
      debugShowCheckedModeBanner: false,
    );
  }
}

class GanjilGenapPage extends StatelessWidget {
  final List<int> a = [1, 1, 2, 3, 5, 8, 13, 21, 4, 55, 89];

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text("Ganjil / Genap")),
      body: ListView.builder(
        itemCount: a.length,
        itemBuilder: (context, index) {
          final angka = a[index];
          final status = angka % 2 == 0 ? "Genap" : "Ganjil";

          return ListTile(
            title: Text("$angka"),
            trailing: Text(status),
          );
        },
      ),
    );
  }
}
